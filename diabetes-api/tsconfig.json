{
    "compilerOptions": {
      "target": "ES2016",
      "module": "commonjs",
      "lib": ["ES6"],
      "outDir": "./build",
      "rootDir": "./src",
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./src","./src/**/*.ts"],
    "exclude": ["node_modules", "spec", "build"]
}